{
    "name": "rafaivideo",
    "version": 2,
    "builds": [{
        "src": "index.js",
        "use": "@vercel/node"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/index.js"
    }],
    "env": {
        "DB_USER": "@rafavideos-db-user",
        "DB_PASSWORD": "@rafavideos-db-password",
        "DB_HOST": "@rafavideos-db-host",
        "DB_NAME": "@rafavideos-db-name"
    }
}
